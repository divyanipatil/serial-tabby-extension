{
  "manifest_version": 3,
  "name": "SerialTabby",
  "version": "1.0",
  "description": "Organize tabs, set reminders, and search efficiently",
  "permissions": [
    "tabs",
    "storage",
    "alarms",
    "notifications"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "icon48.png"
  }
}
